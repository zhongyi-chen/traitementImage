## Makefile PVISU and LIBPANDORE (DYNAMIC LIB)
##-----------------------------------------------------------
## DO NOT EDIT BY HAND
## Autogenerated by ./configure.
## If something is wrong, fix ./configure and run it again. 
##-----------------------------------------------------------
## (2017) Modified by Vinh
##
CPPFLAGS=-Isrc/pandore -DNDEBUG -O3
CXXFLAGS=-Wall -Wextra -Werror -Wpointer-arith -Wwrite-strings -Wcast-qual  -Wformat-security -Wmissing-format-attribute -Wno-overloaded-virtual -s -fpic
LDFLAGS=-shared
LDLIBS= 

OBJ= src/pandore/bundled.o src/pandore/collection.o src/pandore/errc.o src/pandore/graph.o src/pandore/main.o src/pandore/pobject.o src/pandore/region.o
LIB=lib/libpandore.so
PVISU=bin/pvisu

.PHONY: pre
pre:
	mkdir -p bin lib
	$(MAKE) all

.PHONY: all
all: $(LIB) $(PVISU)

$(LIB): $(OBJ)
	 $(RM) $@
	 $(CXX) -o $@ $(LDFLAGS) $^

$(PVISU):
	cd src/pvisu && qmake-qt4 pvisu.pro && $(MAKE)

.PHONY: clean cleanall distclean usage
clean:
	$(RM) $(OBJ)
	$(MAKE) -C src/pvisu clean
cleanall: clean
	$(RM) $(LIB) $(PVISU)
distclean: cleanall
	   $(RM) -r bin lib src/pvisu/.moc src/pvisu/.obj Makefile .qmake.stash

